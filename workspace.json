{
	"version": 2,
	"projects": {
		"cargo": {
			"root": "packages/cargo",
			"sourceRoot": "packages/cargo/src",
			"projectType": "library",
			"targets": {
				"lint": {
					"executor": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["packages/cargo/**/*.ts"]
					}
				},
				"test": {
					"executor": "@nrwl/jest:jest",
					"outputs": ["coverage/packages/cargo"],
					"options": {
						"jestConfig": "packages/cargo/jest.config.js",
						"passWithNoTests": true
					}
				},
				"build": {
					"executor": "@nrwl/node:package",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/packages/cargo",
						"tsConfig": "packages/cargo/tsconfig.lib.json",
						"packageJson": "packages/cargo/package.json",
						"main": "packages/cargo/src/index.ts",
						"assets": [
							"packages/cargo/*.md",
							{
								"input": "./packages/cargo/src",
								"glob": "**/!(*.ts)",
								"output": "./src"
							},
							{
								"input": "./packages/cargo/src",
								"glob": "**/*.d.ts",
								"output": "./src"
							},
							{
								"input": "./packages/cargo",
								"glob": "generators.json",
								"output": "."
							},
							{
								"input": "./packages/cargo",
								"glob": "executors.json",
								"output": "."
							}
						]
					}
				}
			}
		},
		"cargo-e2e": {
			"root": "e2e\\cargo-e2e",
			"projectType": "application",
			"sourceRoot": "e2e\\cargo-e2e/src",
			"targets": {
				"e2e": {
					"executor": "@nrwl/nx-plugin:e2e",
					"options": {
						"target": "cargo:build",
						"npmPackageName": "@nxrs/cargo",
						"pluginOutputPath": "dist/packages/cargo",
						"jestConfig": "e2e/cargo-e2e/jest.config.js"
					}
				}
			}
		}
	},
	"defaultProject": "cargo",
	"cli": {
		"defaultCollection": "@nrwl/nx-plugin"
	}
}
